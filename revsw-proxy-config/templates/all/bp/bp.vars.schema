{
    "title": "Browser Proxy config",
    "type": "object",
    "properties": {
        "VERSION": {
            "type": "integer",
            "minimum": 0,
            "maximum": 29
        },
        "ssl": {% include "common/ssl" %},
        "acl": {% include "bp/acl" %},
        "SERVER_NAME": {
            "type": "string",
            "pattern": "^([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9_\\-]{0,61}[a-zA-Z0-9])(\\.([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9_\\-]{0,61}[a-zA-Z0-9]))*$"
        },
        "SERVER_ALIASES": {
            "type": "array",
            "items": {
                "type": "string",
                "pattern": "^([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9_\\-]{0,61}[a-zA-Z0-9])(\\.([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9_\\-]{0,61}[a-zA-Z0-9]))*$"
            }
        },
        "SERVER_REGEX_ALIAS": {
            "type": "string"
        },
        "ORIGIN_SERVER_NAME": {
            "type": "string",
            "pattern": "^([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9_\\-]{0,61}[a-zA-Z0-9])(\\.([a-zA-Z0-9]|[a-zA-Z0-9][a-zA-Z0-9_\\-]{0,61}[a-zA-Z0-9]))*$"
        },
        "ENABLE_VARNISH": {
            "type": "boolean"
        },
        "REV_PROFILES_COUNT": {
            "type": "integer",
            "minimum": 1,
            "maximum": 4
        },
        "ENABLE_HTTP": {
            "type": "boolean"
        },
        "ENABLE_HTTPS": {
            "type": "boolean"
        },
        "ENABLE_SPDY": {
            "type": "boolean"
        },
        "ENABLE_HTTP2": {
            "type": "boolean"
        },
        "REV_PROFILES_BASE_PORT_HTTP": {
            "type": "integer",
            "minimum": 0,
            "maximum": 65000
        },
        "REV_PROFILES_BASE_PORT_HTTPS": {
            "type": "integer",
            "minimum": 0,
            "maximum": 65000
        },
        "CONTENT_OPTIMIZERS_HTTP": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uri"
            }
        },
        "CONTENT_OPTIMIZERS_HTTPS": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uri"
            }
        },
        "DOMAINS_TO_PROXY_HTTP": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uri"
            }
        },
        "DOMAINS_TO_PROXY_HTTPS": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uri"
            }
        },
        "DOMAINS_TO_OPTIMIZE_HTTP": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uri"
            }
        },
        "DOMAINS_TO_OPTIMIZE_HTTPS": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uri"
            }
        },
        "DOMAIN_SHARDS_COUNT": {
            "type": "integer",
            "minimum": 0
        },
        "CUSTOM_WEBSERVER_CODE_BEFORE": {
            "type": "string"
        },
        "CUSTOM_WEBSERVER_CODE_AFTER": {
            "type": "string"
        },
        "CUSTOM_WEBSERVER_CO_CODE_AFTER": {
            "type": "string"
        },
        "BLOCK_CRAWLERS": {
            "type": "boolean"
        },
        "ENABLE_JS_SUBSTITUTE": {
            "type": "boolean"
        },
        "ENABLE_HTML_SUBSTITUTE": {
            "type": "boolean"
        },
        "DEBUG_MODE": {
        	"type": "boolean"
        },
        "BYPASS_VARNISH_LOCATIONS": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uri"
            }
        },
        "BYPASS_CO_LOCATIONS": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uri"
            }
        },
        "PROXY_TIMEOUT": {
            "type": "integer",
            "minimum": 0
        },
        "ORIGIN_SERVERS_HTTP": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uri"
            }
        },
        "ORIGIN_SERVERS_HTTPS": {
            "type": "array",
            "items": {
                "type": "string",
                "format": "uri"
            }
        },
        "ORIGIN_SECURE_PROTOCOL": {
            "type": "string"
        },
        "ORIGIN_IDLE_TIMEOUT": {
            "type": "integer",
            "minimum": 0
        },
        "ORIGIN_REUSE_CONNS": {
            "type": "boolean"
        },
        "ENABLE_VARNISH_GEOIP_HEADERS": {
            "type": "boolean"
        },
        "ENABLE_PROXY_BUFFERING": {
            "type": "boolean"
        },
        "END_USER_RESPONSE_HEADERS": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "operation": {"type": "string"},
                    "header_name": {"type": "string"},
                    "header_value": {"type": "string"}
                }
            }
        },
        "ENABLE_RUM": {
            "type": "boolean"
        },
        "REV_RUM_BEACON_URL": {
            "type": "string",
            "anyOf": [
                { "format": "uri" },
                { "pattern": "" }
            ]
        },
        "ENABLE_OPTIMIZATION": {
            "type": "boolean"
        },
        "ENABLE_DECOMPRESSION": {
            "type": "boolean"
        },
        "ORIGIN_REQUEST_HEADERS": {
            "type": "array",
            "items": {
		        "type": "object",
		        "properties":
		            {
		                "operation": {"type": "string"},
		                "header_name": {"type": "string"},
		                "header_value": {"type": "string"}
		            }
            }
        },
        "ENABLE_QUIC": {
            "type": "boolean"
        },
        "ENABLE_SSL": {
            "type": "boolean"
        },
        "SSL_PROTOCOLS": {
            "type": "string"
        },
        "SSL_CIPHERS": {
            "type": "string"
        },
        "SSL_PREFER_SERVER_CIPHERS": {
            "type": "boolean"
        },
        "SSL_CERT_ID": {
            "type": "string"
        },
        "BP_LUA_LOCATIONS": {
            "type": "array",
            "items": {
		        "type": "object",
		        "properties":
		            {
		                "location": {"type": "string"},
		                "code": {"type": "string"}
		            }
            }
        },
        "CO_LUA_LOCATIONS": {
            "type": "array",
            "items": {
		        "type": "object",
		        "properties":
		            {
		                "location": {"type": "string"},
		                "code": {"type": "string"}
		            }
            }
        },
        "LUA_LOCATIONS": {
            "type": "array",
            "_comment": "Do not remove"
        },
        "ENABLE_WAF": {
            "type": "boolean"
        },
        "WAF_RULES": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "location": {
                        "type": "string"
                    },
                    "enable_waf": {
                        "type": "boolean"
                    },
                    "enable_learning_mode": {
                        "type": "boolean"
                    },
                    "enable_sql_injection_lib": {
                        "type": "boolean"
                    },
                    "enable_xss_injection_lib": {
                        "type": "boolean"
                    },
                    "waf_rules": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "pattern": "^([a-zA-Z0-9]{24})$"
                        }
                    },
                    "waf_actions": {
                        "type": "array"
                    }
                }
            }
        },
        "ENABLE_BOT_PROTECTION": {
            "type": "boolean"
        },
        "BOT_PROTECTION": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "location": {
                        "type": "string"
                    },
                    "mode": {
                        "type": "string",
                        "pattern": "disable|monitor|active_protection"
                    },
                    "call_type": {
                        "type": "integer",
                        "minimum": 1,
                        "maximum": 6
                    },
                    "username_cookie_name": {
                        "type": "string"
                    },
                    "sessionid_cookie_name": {
                        "type": "string"
                    },
                    "bot_protection_id": {
                        "type": "string"
                    }
                }
            }
        },
        "ENABLE_WALLARM": {
           "type": "boolean"
        },
        "WALLARM_CONFIG": {
            "type": "array",
            "items": {
                "type": "object",
                "properties": {
                    "location": {
                        "type": "string"
                    },
                    "wallarm_mode": {
                        "type": "string",
                        "pattern": "off|monitoring|block|aggressive"
                    },
                    "wallarm_instance": {
                        "type": "integer"
                    },
                    "wallarm_mode_allow_override": {
                        "type": "string",
                        "pattern": "off|strict|on"
                    },
                    "wallarm_parse_response": {
                        "type": "string",
                        "pattern": "on|off"
                    },
                    "wallarm_parser_disable": {
                        "type": "array",
                        "items": {
                            "type": "string",
                            "pattern": "action|cookie|gzip|json|multipart|base64|path|percent|urlenc|xml"
                        }
                    },
                    "wallarm_process_time_limit": {
                        "type": "integer"
                    },
                    "wallarm_process_time_limit_block": {
                        "type": "string",
                        "pattern": "on|off|attack"
                    },
                    "wallarm_unpack_response": {
                        "type": "string",
                        "pattern": "on|off"
                    }
                },
                "required": [
                    "location",
                    "wallarm_mode"
                ]
            }
        },
        "SECURITY_MODE": {
            "type": "string"
        },
        "IP_ADDRESS_ANONYMIZER": {
            "type": "boolean"
        }
    },
    "required": [
        "VERSION",
        "ssl",
        "acl",
        "SERVER_NAME",
        "SERVER_ALIASES",
        "SERVER_REGEX_ALIAS",
        "ORIGIN_SERVER_NAME",
        "ENABLE_VARNISH",
        "REV_PROFILES_COUNT",
        "ENABLE_HTTP",
        "ENABLE_HTTPS",
        "ENABLE_SPDY",
        "REV_PROFILES_BASE_PORT_HTTP",
        "REV_PROFILES_BASE_PORT_HTTPS",
        "CONTENT_OPTIMIZERS_HTTP",
        "CONTENT_OPTIMIZERS_HTTPS",
        "DOMAINS_TO_PROXY_HTTP",
        "DOMAINS_TO_PROXY_HTTPS",
        "DOMAIN_SHARDS_COUNT",
        "CUSTOM_WEBSERVER_CODE_BEFORE",
        "CUSTOM_WEBSERVER_CODE_AFTER",
        "BLOCK_CRAWLERS",
        "ENABLE_JS_SUBSTITUTE",
        "ENABLE_HTML_SUBSTITUTE",
        "DEBUG_MODE",
        "DOMAINS_TO_OPTIMIZE_HTTP",
        "DOMAINS_TO_OPTIMIZE_HTTPS",
        "BYPASS_VARNISH_LOCATIONS",
        "PROXY_TIMEOUT",
        "BYPASS_CO_LOCATIONS",
        "ORIGIN_SERVERS_HTTP",
        "ORIGIN_SERVERS_HTTPS",
        "ORIGIN_SECURE_PROTOCOL",
        "ORIGIN_IDLE_TIMEOUT",
        "ORIGIN_REUSE_CONNS",
        "ENABLE_VARNISH_GEOIP_HEADERS",
        "ENABLE_PROXY_BUFFERING",
        "END_USER_RESPONSE_HEADERS",
        "ENABLE_OPTIMIZATION",
        "ENABLE_DECOMPRESSION",
        "ORIGIN_REQUEST_HEADERS",
        "ENABLE_HTTP2",
        "ENABLE_RUM",
        "REV_RUM_BEACON_URL",
        "CUSTOM_WEBSERVER_CO_CODE_AFTER",
        "ENABLE_QUIC",
        "IP_ADDRESS_ANONYMIZER",

        "ENABLE_SSL",
        "SSL_PROTOCOLS",
        "SSL_CIPHERS",
        "SSL_PREFER_SERVER_CIPHERS",
        "SSL_CERT_ID",
        "ENABLE_WAF",
        "WAF_RULES",
        "ENABLE_BOT_PROTECTION",
        "BOT_PROTECTION",
        "ENABLE_WALLARM",
        "WALLARM_CONFIG"
    ],
    "additionalProperties": false
}
